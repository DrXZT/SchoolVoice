<?xml version="1.0" encoding="UTF-8"?>

    <selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_pressed="true">


    <shape>

    <gradient android:endColor="#ffff00" android:startColor="#ffff00"/>

    <stroke android:color="#ffff00" android:width="4dip"/>

    <corners android:radius="120dp"/>

    <padding android:bottom="10dp" android:right="10dp" android:top="10dp" android:left="10dp"/>

</shape>

</item>


    <item android:state_focused="true">


    <shape>

    <gradient android:endColor="#ffff00" android:startColor="#ffff00"/>

    <stroke android:color="#ffff00" android:width="4dip"/>

    <corners android:radius="120dp"/>

    <padding android:bottom="10dp" android:right="10dp" android:top="10dp" android:left="10dp"/>

</shape>

</item>


    <item>


    <shape>

    <gradient android:endColor="#ffff00" android:startColor="#ffff00"/>

    <stroke android:color="#ffff00" android:width="4dip"/>

    <corners android:radius="120dip"/>

    <padding android:bottom="10dp" android:right="10dp" android:top="10dp" android:left="10dp"/>

</shape>

</item>

</selector>