<?xml version="1.0" encoding="UTF-8"?>

    <selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_pressed="true">


    <shape>

    <gradient android:endColor="#080808" android:startColor="#080808"/>

    <stroke android:color="#080808" android:width="1dip"/>

    <corners android:radius="270dp"/>

    <padding android:bottom="10dp" android:right="10dp" android:top="10dp" android:left="10dp"/>

</shape>

</item>


    <item android:state_focused="true">


    <shape>

    <gradient android:endColor="#080808" android:startColor="#080808"/>

    <stroke android:color="#080808" android:width="1dip"/>

    <corners android:radius="270dp"/>

    <padding android:bottom="10dp" android:right="10dp" android:top="10dp" android:left="10dp"/>

</shape>

</item>


    <item>


    <shape>

    <gradient android:endColor="#080808" android:startColor="#080808"/>

    <stroke android:color="#080808" android:width="1dip"/>

    <corners android:radius="270dip"/>

    <padding android:bottom="10dp" android:right="10dp" android:top="10dp" android:left="10dp"/>

</shape>

</item>

</selector>